type run_one string -> string -> string -> string -> prop.

type prog string -> tm -> prop.

run_one Name Prog Value Type :-
	prog Name P, 
	term_to_string P Prog,
	if (typeof P T)
	   (term_to_string T Type)
  	   (Type is "Type error."),
	if (eval P V)
	   (term_to_string V Value)
	   (Value is "Evaluation failed.")
	.
